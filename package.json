{
  "name": "StackOverFlow-Lite",
  "version": "1.0.0",
  "description": "StackOverFlow-Lite Application is a platform where users can ask questions and get answers to their questions",
  "main": "index.js",
  "scripts": {
    "test": "npm run db:initialize --exit --exit && cross-env NODE_ENV=test nyc --require babel-register --require babel-polyfill mocha --timeout 1000000 server/tests/**/*.js --exit",
    "start": "babel-node server/app.js",
    "db:initialize": "cross-env NODE_ENV=test babel-node server/migrations/index.js --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text --require babel-register --require babel-polyfill mocha server/tests/**/*.js --exit "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/augustineezinwa/StackOverFlow-Lite.git"
  },
  "keywords": [],
