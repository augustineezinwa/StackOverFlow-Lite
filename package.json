  "license": "ISC",
  "bugs": {
    "url": "https://github.com/augustineezinwa/StackOverFlow-Lite/issues"
  },
  "homepage": "https://github.com/augustineezinwa/StackOverFlow-Lite#readme",
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cors": "^2.8.4",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.3.0",
    "mocha": "^5.2.0",
    "morgan": "^1.9.0",
    "nyc": "^12.0.2",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  }
}
